<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee  
                     http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
 metadata-complete="true">
 <display-name>WebSocketTest</display-name>
 <servlet>
    <servlet-name>echo</servlet-name>
    <servlet-class>org.sample.server.EchoServlet</servlet-class>
 </servlet>
 <servlet-mapping>
    <servlet-name>echo</servlet-name>
    <url-pattern>/echo</url-pattern>
 </servlet-mapping>

 <servlet>
    <servlet-name>noecho</servlet-name>
    <servlet-class>org.sample.server.NoEchoServlet</servlet-class>
 </servlet>
 <servlet-mapping>
    <servlet-name>noecho</servlet-name>
    <url-pattern>/noecho</url-pattern>
 </servlet-mapping>
</web-app>
